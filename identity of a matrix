#include<stdio.h>
int main()
{
	int a[10][10],b[10][10],m,n,l,k,i,j;
	printf("Enter the order of 1st  matrix\n");
	scanf("%d %d",&m,&n);
	printf("Enter the 1st matrix\n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			scanf("%d",&a[i][j]);
		}
	}
	printf("Matrix is\n");
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++)
		{
			printf("%d\t",a[i][j]);
		}
		printf("\n");
	}
		int s=0;
		for(i=0;i<m;i++){
			s=s+a[i][i];
		}
 		if(s==3)
 		printf("identity");
 		else
 		printf("NOt identity");
 	
	return 0;
	
}
