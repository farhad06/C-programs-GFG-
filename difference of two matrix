#include<bits/stdc++.h>
using namespace std;
int main(){
	int a[10][10],b[10][10],t[10][10],r,c,i,j;
	cout<<"Enter the numbers of row\n";
	cin>>r;
	cout<<"Enter the numbers of column\n";
	cin>>c;
	cout<<"Enter the  Matrix\n";
	for(i=0;i<r;i++)
	for(j=0;j<c;j++)
	cin>>a[i][j];
   for(i=0;i<r;i++){
   	for(j=0;j<c;j++){
   		t[i][j]=a[j][i];
	   }
   }
   	cout<<"\n\n\nTranspose of  Matrix is:\n\n";
	for(i=0;i<r;i++){
	for(j=0;j<c;j++){
		cout<<t[i][j]<<"\t";
	}
	cout<<endl;
   }	
	return 0;
}
