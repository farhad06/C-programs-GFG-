#include<iostream>
using namespace std;
int main(){
	int a[10],b[10],i,j,n;
	cout<<"enter the number of element"<<endl;
	cin>>n;
	cout<<"enter the array list\n";
	for(i=0;i<n;i++)
	cin>>a[i];
	int min=a[0];
	int max=a[0];
	for(i=0;i<n;i++){
		if(a[i]>max){
			max=a[i];
		}else{
			min=a[i];
		}
	}
	cout<<max<<endl<<min;
	return 0;
}
